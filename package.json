{
  "name": "phccscode",
  "displayName": "PHC Cs Code",
  "description": "Extensão para apoio ao desenvolvimento na PHC CS Framework.",
  "publisher": "miguel17lemos",
  "version": "1.5.0",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [     
      {
        "command": "phccscode.pull",
        "title": "phccscode: Pull"
      },
      {
        "command": "phccscode.push",
        "title": "phccscode: Push"
      },
      {
        "command": "phccscode.push_activate_text_editor",
        "title": "phccscode: Submit Current File"
      },
      {
        "command": "phccscode.settings",
        "title": "phccscode: Settings"
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "phccscode.ConnectionString": {
          "type": "string",
          "default": "Server=localhost,1433;Database=database;User Id=username;Password=password;Encrypt=false",
          "description": "Connection String"
        },
        "phccscode.PhcURL": {
          "type": "string",
          "default": "http://PHC/Intranet",
          "description": "URL do PHC"
        },
        "phccscode.PhcSessionCookies": {
          "type": "string",
          "default": "ASP.NET_SessionId=XXXXXXXXXX; PHC_Intranet202404=XXXXXXXXXX",
          "description": "Cookies de sessão atual PHC"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "mssql": "^11.0.1"
  }
}
